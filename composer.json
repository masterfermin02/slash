{
  "name": "masterfermin02/slash",
  "description": "A functional programming library for PHP",
  "keywords": [
    "iterator",
    "generator",
    "functional",
    "PHP",
    "library",
    "programming",
    "functional programming",
    "underscore",
    "lodash",
    "underbar",
    "bottomline",
    "ramda"
  ],
  "homepage": "https://github.com/masterfermin02/slash",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Fermin Perdomo",
      "email": "masterfermin02@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.13.2",
    "friendsofphp/php-cs-fixer": "^3.75",
    "phpstan/phpstan": "^1.12.27|^2.0",
    "rector/rector": "^2.0.18",
    "phpunit/phpunit": "^12.2",
    "rector/jack": "^0.2.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Slash\\": [
          "src/"
        ]
    },
    "files": [
      "src/all.php",
      "src/any.php",
      "src/average.php",
      "src/comparator.php",
      "src/compose.php",
      "src/curryLeft.php",
      "src/curryRight.php",
      "src/equal.php",
      "src/equalTo.php",
      "src/filter.php",
      "src/filterWith.php",
      "src/first.php",
      "src/firstWith.php",
      "src/flatMap.php",
      "src/flatMapWith.php",
      "src/flatten.php",
      "src/flip.php",
      "src/get.php",
      "src/getWith.php",
      "src/greaterThan.php",
      "src/greaterThanOrEqual.php",
      "src/greaterThanOrEqualTo.php",
      "src/group.php",
      "src/groupBy.php",
      "src/helps.php",
      "src/isEven.php",
      "src/isOdd.php",
      "src/last.php",
      "src/lessThan.php",
      "src/map.php",
      "src/mapWith.php",
      "src/pair.php",
      "src/pairWith.php",
      "src/pipeLine.php",
      "src/pluck.php",
      "src/pluckWith.php",
      "src/reduce.php",
      "src/reduceWith.php",
      "src/reject.php",
      "src/rejectWith.php",
      "src/sort.php",
      "src/sortBy.php",
      "src/sum.php",
      "src/useWith.php",
      "src/unique.php",
      "src/walk.php"
    ]
  },
  "scripts": {
    "test:types": "phpstan analyse --memory-limit=2g",
    "refactor": "rector",
    "test:refactor": "rector --dry-run",
    "test": "vendor/bin/phpunit"
  }
}
