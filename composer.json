{
  "name": "masterfermin02/slash",
  "description": "A functional programming library for PHP",
  "keywords": [
    "iterator",
    "generator",
    "functional",
    "PHP",
    "library",
    "programming",
    "functional programming",
    "underscore",
    "lodash",
    "underbar",
    "bottomline",
    "ramda"
  ],
  "homepage": "https://github.com/masterfermin02/slash",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Fermin Perdomo",
      "email": "masterfermin02@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpdocumentor/phpdocumentor": "^2.8",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Slash\\": [
          "src/"
        ]
    },
    "files": [
      "src/all.php",
      "src/any.php",
      "src/comparator.php",
      "src/compose.php",
      "src/curryLeft.php",
      "src/curry_right.php",
      "src/equal.php",
      "src/equalTo.php",
      "src/filter.php",
      "src/filterWith.php",
      "src/first.php",
      "src/firstWith.php",
      "src/flatMap.php",
      "src/flatMapWith.php",
      "src/flatten.php",
      "src/flip.php",
      "src/get.php",
      "src/getWith.php",
      "src/greaterThan.php",
      "src/greaterThanOrEqual.php",
      "src/greaterThanOrEqualTo.php",
      "src/group.php",
      "src/groupBy.php",
      "src/isEven.php",
      "src/isOdd.php",
      "src/isType.php",
      "src/last.php",
      "src/lessThan.php",
      "src/map.php",
      "src/mapObject.php",
      "src/mapObjectWith.php",
      "src/pair.php",
      "src/pairWith.php",
      "src/pipeLine.php",
      "src/pluck.php",
      "src/pluckWith.php",
      "src/reduce.php",
      "src/reduceWith.php",
      "src/sort.php",
      "src/sortBy.php",
      "src/useWith.php",
      "src/walk.php"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
